<script type="text/javascript">
    function showImage(imageUrl) {
        var imagePreview1 = document.getElementById("image-preview2");
        var imageLink = document.getElementById("image-link");
        var imageCaption1 = document.getElementById("image-caption");

        imagePreview1.src = imageUrl;
        imageLink.href = imageUrl;

        if (imageUrl === "big.png") {
            imageCaption1.innerHTML = "è¿™æ˜¯aå›¾ç‰‡";
        } else if (imageUrl === "down.png") {
            imageCaption1.innerHTML = "ç»“æŸäº†";
        }
    }

    window.addEventListener('DOMContentLoaded', (event) => {
        var imagePreview1 = document.getElementById("image-preview");
        var imageLink = document.getElementById("image-link");
        var rightImage = document.getElementById("image-1");

        imagePreview1.src = rightImage.src;
        imageLink.href = rightImage.src;
        document.getElementById("image-caption").innerHTML = "è¿™æ˜¯aå›¾ç‰‡";

        var zoomArea = document.getElementById("image-preview");
        var panzoom = Panzoom(zoomArea, {
            maxScale: 6
        });
        zoomArea.addEventListener("wheel", function(e) {
            e.preventDefault();
            panzoom.zoomWithWheel(e);
        });
        panzoom.zoom(0.8);
    });
</script>


