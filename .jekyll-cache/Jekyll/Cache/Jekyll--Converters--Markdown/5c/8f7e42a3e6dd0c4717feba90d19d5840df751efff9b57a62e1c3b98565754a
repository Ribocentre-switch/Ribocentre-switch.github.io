    function showImage(imageUrl) {
        var imagePreview = document.getElementById("image-preview");
        var imageLink0 = document.getElementById("image-link");
        var imageCaption0 = document.getElementById("image-caption");

        imagePreview.src = imageUrl;
        imageLink0.href = imageUrl;

        if (imageUrl === "big.png") {
            imageCaption0.innerHTML = "è¿™æ˜¯aå›¾ç‰‡";
        } else if (imageUrl === "down.png") {
            imageCaption.innerHTML = "ç»“æŸäº†";
        }
    }

    window.addEventListener('DOMContentLoaded', (event) => {
        var imagePreview = document.getElementById("image-preview");
        var imageLink0 = document.getElementById("image-link");
        var rightImage = document.getElementById("image-a");

        imagePreview.src = rightImage.src;
        imageLink0.href = rightImage.src;
        document.getElementById("image-caption").innerHTML = "è¿™æ˜¯aå›¾ç‰‡";

        var zoomArea = document.getElementById("image-preview");
        var panzoom = Panzoom(zoomArea, {
            maxScale: 6
        });
        zoomArea.addEventListener("wheel", function(e) {
            e.preventDefault();
            panzoom.zoomWithWheel(e);
        });
        panzoom.zoom(0.8);
    });
</script>


