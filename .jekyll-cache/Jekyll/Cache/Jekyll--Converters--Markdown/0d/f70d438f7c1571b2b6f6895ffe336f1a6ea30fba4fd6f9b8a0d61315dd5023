I"¡<html>
<head>
  <style>
    /* æ ·å¼è¡¨æ ¼ */
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      border: 1px solid black;
      padding: 8px;
      text-align: left;
    }

    th {
      background-color: #f2f2f2;
    }

    /* éšè—æ‰€æœ‰ sheet */
    .sheet {
      display: none;
    }
  </style>
</head>
<body onload="showSheet('sheet1')">
  <!-- æœç´¢æ¡† -->
  <input type="text" id="searchBox" placeholder="æœç´¢å…³é”®å­—..." oninput="searchSheets()" />

  <!-- æŒ‰é’® -->
  <button onclick="showSheet('sheet1')">Sheet 1</button>
  <button onclick="showSheet('sheet2')">Sheet 2</button>

  <!-- è¡¨æ ¼ -->
  <div id="sheet1" class="sheet">
    <h2>Sheet 1</h2>
    <table>
      <tr>
        <th>Header 1</th>
        <th>Header 2</th>
        <th>Header 3</th>
      </tr>
      <tr>
        <td>Data 1</td>
        <td>Data 2</td>
        <td>Data 3</td>
      </tr>
      <!-- å…¶ä»–è¡Œ... -->
    </table>
  </div>

  <div id="sheet2" class="sheet">
    <h2>Sheet 2</h2>
    <table>
      <tr>
        <th>Header 1</th>
        <th>Header 2</th>
        <th>Header 3</th>
      </tr>
      <tr>
        <td>Data 1</td>
        <td>Data 2</td>
        <td>Data 3</td>
      </tr>
      <!-- å…¶ä»–è¡Œ... -->
    </table>
  </div>

  <script>
    function showSheet(sheetId) {
      // éšè—æ‰€æœ‰ sheet
      var sheets = document.getElementsByClassName('sheet');
      for (var i = 0; i < sheets.length; i++) {
        sheets[i].style.display = 'none';
      }

      // æ˜¾ç¤ºé€‰å®šçš„ sheet
      var sheet = document.getElementById(sheetId);
      sheet.style.display = 'block';
    }

    function searchSheets() {
      var searchBox = document.getElementById('searchBox');
      var keyword = searchBox.value.toLowerCase();

      var sheets = document.getElementsByClassName('sheet');
      for (var i = 0; i < sheets.length; i++) {
        var sheet = sheets[i];
        var sheetTitle = sheet.getElementsByTagName('h2')[0].textContent.toLowerCase();

        if (sheetTitle.includes(keyword)) {
          sheet.style.display = 'block';
        } else {
          sheet.style.display = 'none';
        }
      }
    }
  </script>
</body>
</html>
